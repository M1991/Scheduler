@model IEnumerable<Scheduler.Models.HolidayTableModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.incr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hdate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HDesc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.incr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hdate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HDesc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.iId }) |
            @Html.ActionLink("Details", "Details", new { id=item.iId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.iId })
        </td>
    </tr>
}

</table>
